          "schema": {
            "type": "integer",
            "format": "int",
            "enum": [
              1
            ]
          }
        },
        {
          "name": "start",
          "description": "Optional start date for filtering",
          "in": "query",
          "required": false,
          "schema": {
            "type": "string",
            "format": "date"
          }
        }
      ],
      "get": {
        "tags": [
          "openmensa"
        ],
        "summary": "List days of canteen",
        "responses": {
