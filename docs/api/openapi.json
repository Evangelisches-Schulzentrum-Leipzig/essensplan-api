                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "404": {
            "description": "Unknown canteen",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/openmensa/api-v2/canteens/{canteen_id}/days/{date}": {
      "parameters": [
        {
          "name": "canteen_id",
          "description": "ID of canteen",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer",
            "format": "int",
            "enum": [
              1
            ]
          }
        },
        {
          "name": "date",
          "description": "date of day",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "date"
          }
        }
      ],
      "get": {
        "tags": [
          "openmensa"
        ],
