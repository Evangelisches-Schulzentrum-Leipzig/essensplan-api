            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Supplement"
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/supplements/{id}": {
      "parameters": [
        {
          "name": "id",
          "description": "ID of a specific supplement",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "enum": [
              "A",
              "B",
              "C"
            ]
          }
        }
      ],
      "get": {
        "tags": [
          "specifics"
        ],
        "summary": "Returns information on a specific supplement",
        "responses": {
          "200": {
            "description": "Supplement details",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Supplement"
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/menues": {
      "get": {
        "tags": [
          "specifics"
        ],
        "summary": "Returns list of all known menues/meal categories",
        "responses": {
          "200": {
            "description": "List of all menu categories",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Category"
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/meals": {
      "get": {
        "tags": [
          "meals"
        ],
        "summary": "Query all known meals",
        "responses": {
          "200": {
            "description": "List of all meals",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Meal"
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/meals/{mealID}": {
      "parameters": [
        {
          "name": "mealID",
          "description": "ID of a specific meal",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer",
            "format": "int"
          }
        }
      ],
      "get": {
        "tags": [
          "meals"
        ],
        "summary": "Return information on a specific meal",
        "responses": {
          "200": {
            "description": "Meal details",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Meal"
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/weeks/{date}": {
      "parameters": [
        {
          "name": "date",
          "description": "Date in requested week",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "date-time"
          }
        }
      ],
      "get": {
        "tags": [
          "plans"
        ],
        "summary": "Return weekly meal plan for a given date in that week",
        "description": "Return weekly meal plan for a given date in that week",
        "responses": {
          "200": {
            "description": "Weekly meal plan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DateRangePlan"
                }
              }
            }
          },
          "400": {
            "description": "Invalid date format",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/days/{date}": {
      "parameters": [
        {
          "name": "date",
          "description": "Date in requested week",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "date-time"
          }
        }
      ],
      "get": {
        "tags": [
          "plans"
        ],
        "summary": "Return daily meal plan for a given date",
        "description": "Return daily meal plan for a given date",
        "responses": {
          "200": {
            "description": "Daily meal plan",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/DailyPlanEntry"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid date format",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/plan-range/{startDate}/{endDate}": {
      "parameters": [
        {
          "name": "startDate",
          "description": "Starting date for plan range",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "date-time"
          }
        },
        {
          "name": "endDate",
          "description": "Ending date for plan range",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "date-time"
          }
        }
      ],
      "get": {
        "tags": [
          "plans"
        ],
        "summary": "Return meal plan for a given date range",
        "description": "Return meal plan for a given date range",
        "responses": {
          "200": {
            "description": "Meal plan for the date range",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DateRangePlan"
                }
              }
            }
          },
          "400": {
            "description": "Invalid date format, invalid date range, or range limit exceeded",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/plans": {
      "get": {
        "tags": [
          "plans"
        ],
        "summary": "Return meal plans for all days from today on that have a meal plan",
        "description": "Return meal plans for all days from today on that have a meal plan",
        "responses": {
          "200": {
            "description": "Meal plans from today onwards",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DateRangePlan"
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/plans/{relativeDayOffset}": {
      "parameters": [
        {
          "name": "relativeDayOffset",
          "description": "Relative day offset from today",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer",
            "format": "int",
            "min": -999,
            "max": 999
          }
        }
      ],
      "get": {
        "tags": [
          "plans"
        ],
        "summary": "Return meal plans for all days from today until day offset",
        "description": "Return meal plans for all days from today until day offset including future and previous days which have a meal plan",
        "responses": {
          "200": {
            "description": "Meal plans for the offset range",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DateRangePlan"
                }
              }
            }
          },
          "400": {
            "description": "Invalid range limit exceeded",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/available-days/{year}": {
      "parameters": [
        {
          "name": "year",
          "description": "Year for which available dates are requested",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer",
            "format": "integer"
          }
        }
      ],
      "get": {
        "tags": [
          "metadata"
        ],
        "summary": "Returns list of dates for which meal plans are available in a given year",
        "responses": {
          "200": {
            "description": "List of available dates in the given year",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "format": "date"
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/years": {
      "get": {
        "tags": [
          "metadata"
        ],
        "summary": "Returns list of years for which meal plans are available",
        "responses": {
          "200": {
            "description": "List of available years",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "integer"
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/openmensa/feed-v2": {
      "get": {
        "tags": [
          "openmensa"
        ],
        "summary": "OpenMensa parser feed endpoint",
        "description": "Returns OpenMensa v2.1 compatible XML feed with canteen information and meal plans",
        "externalDocs": {
          "description": "Find out more about OpenMensa parser feed",
          "url": "https://docs.openmensa.org/feed/v2/"
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/xml": {
                "schema": {
                  "type": "object",
                  "xml": {
                    "name": "openmensa",
                    "namespace": "http://openmensa.org/open-mensa-v2"
                  },
                  "properties": {
                    "version": {
                      "type": "string",
                      "description": "Feed version",
                      "examples": [
                        "1.0.0"
                      ]
                    },
                    "canteen": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string",
                          "examples": [
                            "Mensa Evangelisches Schulzentrum Leipzig"
                          ]
                        },
                        "address": {
                          "type": "string",
                          "examples": [
                            "Schletterstraße 7, 04107 Leipzig, Deutschland"
                          ]
                        },
                        "city": {
                          "type": "string",
                          "examples": [
                            "Leipzig"
                          ]
                        },
                        "location": {
                          "type": "object",
                          "properties": {
                            "latitude": {
                              "type": "string",
                              "xml": {
                                "attribute": true
                              },
                              "examples": [
                                "51.3303829274839"
                              ]
                            },
                            "longitude": {
                              "type": "string",
                              "xml": {
                                "attribute": true
                              },
                              "examples": [
                                "12.377177720801583"
                              ]
                            }
                          }
                        },
                        "availability": {
                          "type": "string",
                          "examples": [
                            "restricted",
                            "public"
                          ]
                        },
                        "times": {
                          "type": "object",
                          "properties": {
                            "type": {
                              "type": "string",
                              "xml": {
                                "attribute": true
                              },
                              "examples": [
                                "opening"
                              ]
                            },
                            "monday": {
                              "type": "object",
                              "properties": {
                                "open": {
                                  "type": "string",
                                  "xml": {
                                    "attribute": true
                                  },
                                  "examples": [
                                    "09:15-14:00"
                                  ]
                                }
                              }
                            },
                            "tuesday": {
                              "type": "object",
                              "properties": {
                                "open": {
                                  "type": "string",
                                  "xml": {
                                    "attribute": true
                                  },
                                  "examples": [
                                    "09:15-14:00"
                                  ]
                                }
                              }
                            },
                            "wednesday": {
                              "type": "object",
                              "properties": {
                                "open": {
                                  "type": "string",
                                  "xml": {
                                    "attribute": true
                                  },
                                  "examples": [
                                    "09:15-14:00"
                                  ]
                                }
                              }
                            },
                            "thursday": {
                              "type": "object",
                              "properties": {
                                "open": {
                                  "type": "string",
                                  "xml": {
                                    "attribute": true
                                  },
                                  "examples": [
                                    "09:15-14:00"
                                  ]
                                }
                              }
                            },
                            "friday": {
                              "type": "object",
                              "properties": {
                                "open": {
                                  "type": "string",
                                  "xml": {
                                    "attribute": true
                                  },
                                  "examples": [
                                    "09:15-14:00"
                                  ]
                                }
                              }
                            },
                            "saturday": {
                              "type": "object",
                              "properties": {
                                "closed": {
                                  "type": "string",
                                  "xml": {
                                    "attribute": true
                                  },
                                  "examples": [
                                    "true"
                                  ]
                                }
                              }
                            },
                            "sunday": {
                              "type": "object",
                              "properties": {
                                "closed": {
                                  "type": "string",
                                  "xml": {
                                    "attribute": true
                                  },
                                  "examples": [
                                    "true"
                                  ]
                                }
                              }
                            }
                          }
                        },
                        "day": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "xml": {
                              "name": "day"
                            },
                            "properties": {
                              "date": {
                                "type": "string",
                                "xml": {
                                  "attribute": true
                                },
                                "format": "date",
                                "examples": [
                                  "2025-12-15",
                                  "2026-01-17"
                                ]
                              },
                              "category": {
                                "type": "array",
                                "items": {
                                  "type": "object",
                                  "xml": {
                                    "name": "category"
                                  },
                                  "properties": {
                                    "name": {
                                      "type": "string",
                                      "xml": {
                                        "attribute": true
                                      },
                                      "examples": [
                                        "Menü 1",
                                        "Menü 2",
                                        "veg. Menü 3",
                                        "Glutenfrei"
                                      ]
                                    },
                                    "meal": {
                                      "type": "array",
                                      "items": {
                                        "type": "object",
                                        "xml": {
                                          "name": "meal"
                                        },
                                        "properties": {
                                          "name": {
                                            "type": "string",
                                            "examples": [
                                              "Rührei mit Rahmspinat und Salzkartoffeln"
                                            ]
                                          },
                                          "note": {
                                            "type": "array",
                                            "items": {
                                              "type": "string",
                                              "examples": [
                                                "Glutenhaltige Getreide",
                                                "Eier- und Eiererzeugnisse"
                                              ]
                                            }
                                          },
                                          "price": {
                                            "type": "array",
                                            "items": {
                                              "type": "object",
                                              "properties": {
                                                "role": {
                                                  "type": "string",
                                                  "xml": {
                                                    "attribute": true
                                                  },
                                                  "enum": [
                                                    "pupils",
                                                    "other"
                                                  ]
                                                },
                                                "name": {
                                                  "type": "number",
                                                  "xml": {
                                                    "name": "#content"
                                                  },
                                                  "examples": [
                                                    1.2
                                                  ]
                                                }
                                              }
                                            }
                                          }
                                        }
                                      }
                                    }
                                  }
                                }
                              },
                              "closed": {
                                "type": "boolean",
                                "description": "Only without categories and meals when canteen is closed that day",
                                "xml": {
                                  "name": "closed"
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/openmensa/api-v2/canteens": {
      "get": {
        "tags": [
